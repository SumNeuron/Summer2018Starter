Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ExtractListStructure", "::", "usage"}], "=", 
   RowBox[{"StringJoin", "[", "\[IndentingNewLine]", 
    RowBox[{
    "\"\<ExtractListStructure[list] returns a list of tuples containing\>\"", 
     ",", "\[IndentingNewLine]", 
     "\"\<{element, absolute index, level, relative index, last element at \
level instance}\>\""}], "\[IndentingNewLine]", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ExtractListStructure", "[", 
    RowBox[{"ll_", ",", 
     RowBox[{"atomQ_:", " ", 
      RowBox[{"Not", "@*", "ListQ"}]}]}], "]"}], ":=", "\[IndentingNewLine]", 
   
   RowBox[{"Block", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", "\[IndentingNewLine]", 
       RowBox[{"prev", "=", "None"}]}], "\[IndentingNewLine]", "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Reap", "[", "\[IndentingNewLine]", 
       RowBox[{"MapIndexed", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Function", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"atomQ", "@", "#"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{"ListQ", "@", "prev"}], ",", 
                RowBox[{
                 RowBox[{
                  RowBox[{"prev", "[", 
                   RowBox[{"[", "5", "]"}], "]"}], "=", "False"}], ";", 
                 RowBox[{"Sow", "@", "prev"}]}]}], "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"prev", "=", 
               RowBox[{"{", 
                RowBox[{"#", ",", 
                 RowBox[{"i", "++"}], ",", 
                 RowBox[{"Length", "@", "#2"}], ",", 
                 RowBox[{"Last", "@", "#2"}], ",", " ", "None"}], "}"}]}]}], 
             ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{"ListQ", "@", "prev"}], ",", 
               RowBox[{
                RowBox[{
                 RowBox[{"prev", "[", 
                  RowBox[{"[", "5", "]"}], "]"}], "=", "True"}], ";", 
                RowBox[{"Sow", "@", "prev"}], ";", 
                RowBox[{"prev", "=", "None"}]}]}], "]"}]}], 
            "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", "#"}], 
          "\[IndentingNewLine]", "]"}], ",", "ll", ",", "All"}], 
        "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", "]"}], "[", 
      RowBox[{"[", 
       RowBox[{"2", ",", "1"}], "]"}], "]"}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.739951354815884*^9, 3.739951439117269*^9}},
